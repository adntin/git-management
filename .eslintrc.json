{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off",
    "no-var": "error",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "object-curly-newline": ["error", { "multiline": true }]
  }
}
